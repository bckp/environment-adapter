    "type": "library",
    "require-dev": {
        "phpstan/phpstan": "^0.12.94",
        "vimeo/psalm": "^4.9",
        "nette/tester": "^2.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bckp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Radovan Kepak",
            "email": "radovan@kepakovi.cz"
        }
    ],
    "extra": {
